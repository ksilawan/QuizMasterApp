<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.quizmasterapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizMasterApp">
        <activity android:name=".DisplayGrades"/>
        <activity android:name=".InputGrades" />
        <activity android:name=".ClassListGrades"/>
        <activity android:name=".QuizOverPage" />
        <activity android:name=".QuizPage"/>
        <activity android:name=".TeacherMainMenu"/>
        <activity android:name=".TeacherLogIn" />
        <activity android:name=".StudentGradesDisplayPage" />
        <activity android:name=".StudentLogIn" />
        <activity android:name=".StudentMainMenu" />
        <activity android:name=".OptionPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>